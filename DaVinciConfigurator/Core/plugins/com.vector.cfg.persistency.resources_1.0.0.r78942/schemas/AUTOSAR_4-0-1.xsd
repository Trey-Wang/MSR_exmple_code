               <xsd:documentation>Reference to the Ipdu for which the I-Pdu triggering is defined. One I-Pdu can be triggered on different channels (PduR fan-out).</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:simpleContent>
                  <xsd:extension base="AR:REF">
                     <xsd:attribute name="BASE" type="AR:IDENTIFIER--SIMPLE" use="optional"/>
                     <xsd:attribute name="DEST" type="AR:I-PDU--SUBTYPES-ENUM" use="required"/>
                  </xsd:extension>
               </xsd:simpleContent>
            </xsd:complexType>
         </xsd:element>
         <xsd:element maxOccurs="1" minOccurs="0" name="I-SIGNAL-TRIGGERINGS">
