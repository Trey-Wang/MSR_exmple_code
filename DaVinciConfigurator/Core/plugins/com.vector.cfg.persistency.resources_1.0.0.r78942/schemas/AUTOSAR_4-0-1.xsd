               <xsd:simpleContent>
                  <xsd:extension base="AR:REF">
                     <xsd:attribute name="BASE" type="AR:IDENTIFIER--SIMPLE" use="optional"/>
                     <xsd:attribute name="DEST" type="AR:SOCKET-ADDRESS--SUBTYPES-ENUM" use="required"/>
                  </xsd:extension>
