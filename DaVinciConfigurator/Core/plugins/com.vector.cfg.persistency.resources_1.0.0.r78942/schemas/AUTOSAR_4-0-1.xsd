                  </xsd:extension>
               </xsd:simpleContent>
            </xsd:complexType>
         </xsd:element>
         <xsd:element maxOccurs="1" minOccurs="0" name="HANDLE-OUT-OF-RANGE" type="AR:HANDLE-OUT-OF-RANGE-ENUM">
