                              <xsd:attribute name="BASE" type="AR:IDENTIFIER--SIMPLE" use="optional"/>
                              <xsd:attribute name="DEST" type="AR:COMMUNICATION-CONNECTOR--SUBTYPES-ENUM" use="required"/>
