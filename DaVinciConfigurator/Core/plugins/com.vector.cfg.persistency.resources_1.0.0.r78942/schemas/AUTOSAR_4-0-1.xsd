               <xsd:choice maxOccurs="unbounded" minOccurs="0">
                  <xsd:element name="FLEXRAY-COMMUNICATION-CONTROLLER-CONDITIONAL" type="AR:FLEXRAY-COMMUNICATION-CONTROLLER-CONDITIONAL"/>
               </xsd:choice>
