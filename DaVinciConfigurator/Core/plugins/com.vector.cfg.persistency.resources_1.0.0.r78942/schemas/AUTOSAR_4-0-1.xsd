   <!-- element group for class FlatMap::FlatMap -->
   <xsd:group name="FLAT-MAP">
      <xsd:annotation>
         <xsd:documentation>Contains a flat list of references to software objects. This list is used to identify instances and to resolve name conflicts. The scope is given by the RootSwCompositionPrototype for which it is used, i.e. it can be applied to a system, sub-system or ECU-extract.</xsd:documentation>
         <xsd:appinfo source="stereotypes">atpIdentifiable,atpObject</xsd:appinfo>
