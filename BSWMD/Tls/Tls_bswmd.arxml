									<SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
									<DEFAULT-VALUE>20</DEFAULT-VALUE>
									<MAX>3600</MAX>
									<MIN>1</MIN>
								</ECUC-INTEGER-PARAM-DEF>
								<ECUC-BOOLEAN-PARAM-DEF UUID="1afbab2b-dacd-4117-b2db-47159f3a99ae">
									<SHORT-NAME>TlsSupportMasterSecretAccess</SHORT-NAME>
									<LONG-NAME>
										<L-4 L="EN">Support Master Secret Access</L-4>
									</LONG-NAME>
									<DESC>
