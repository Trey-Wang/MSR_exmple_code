										<DESTINATION-REF DEST="ECUC-PARAM-CONF-CONTAINER-DEF">/MICROSAR/WdgIf/WdgIfStateCombiner/WdgIfStateCombinerSlave</DESTINATION-REF>
									</DESTINATION-REFS>
								</ECUC-CHOICE-REFERENCE-DEF>
							</REFERENCES>
						</ECUC-PARAM-CONF-CONTAINER-DEF>
