														<SD GID="DV:BaseUnit">SEC</SD>
														<SD GID="DV:Unit">MSEC</SD>
													</SDG>
													<SDG GID="MSR:Process">
														<SD GID="MSR:TraceRef">SPEC-19571,SPEC-50013</SD>
													</SDG>
												</SDGS>
											</ADMIN-DATA>
